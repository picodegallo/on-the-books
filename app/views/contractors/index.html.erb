<div class="container">
  <div class="navbreak"></div>
    <div class="row-fluid">
      <div class="span12">
<h1> Contractors </h1>
<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Focus</th>
      <th>Available</th>
      <th>More</th>
    </tr>
  </thead>
  <tbody>

<% @contractors.each do |contractor| %>
  <tr>
    <td><%= contractor.id %></td>
    <td><%= contractor.first_name %></td>
    <td><%= contractor.last_name %></td>
    <td><%= contractor.focus %></td>
    <td><%= contractor.is_available %></td>
    <td><%= link_to 'Show', contractor %></td>
    <!-- <td><%#= link_to 'Edit', edit_contractor_path(contractor) %></td>
    <td><%#= link_to 'Delete', contractor, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
  </tr>
<% end %>

  </tbody>
</table>

<br />

<%= link_to 'New Contractor', new_contractor_path %>

      </div>
    </div>
  </div>
</div>
